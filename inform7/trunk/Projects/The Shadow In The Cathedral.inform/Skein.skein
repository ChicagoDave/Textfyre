  <item nodeId="node-0x02E1A398">
    <command xml:space="preserve">get keys</command>
    <result xml:space="preserve">Brother Horloge puts out a worn-out old hand. "Now don't you wander off with my keys," he says. "They're very important."&#x000A;&#x000A;&gt;</result>
