                                        <GradientStop Color="#FF668499" Offset="0.325"/>
                                        <GradientStop Color="#FF244E6D" Offset="0.325"/>
                                        <GradientStop Color="#FF4F7189" Offset="1"/>
                                    </LinearGradientBrush>
                                </Rectangle.Stroke>
                            </Rectangle>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

