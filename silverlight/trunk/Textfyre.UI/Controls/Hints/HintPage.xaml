<UserControl x:Class="Textfyre.UI.Controls.Hints.HintPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    >
    <Grid  Width="280" x:Name="LayoutRoot">
        <StackPanel x:Name="MainPanel">
            <TextBlock x:Name="TxtTitle" Margin="0,0,0,5" TextAlignment="Center" />
            <StackPanel x:Name="HintsPanel"></StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
