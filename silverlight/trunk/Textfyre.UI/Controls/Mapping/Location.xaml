<UserControl x:Class="Textfyre.UI.Controls.Mapping.Location"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    >
    <Grid x:Name="LayoutRoot">
        <Rectangle x:Name="LocationRect" RadiusX="8" RadiusY="8" Fill="#FFBDA892" Stroke="#FF5D4832" StrokeThickness="2" />
        <TextBlock x:Name="Title" Text="Location" Foreground="White" Margin="4,4,4,4" TextAlignment="Center" TextWrapping="Wrap" />
        <Line x:Name="LineTest" />
    </Grid>
</UserControl>
