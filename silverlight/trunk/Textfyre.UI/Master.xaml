<UserControl x:Class="Textfyre.UI.Master"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
    Width="920" Height="600" >
    <Grid x:Name="LayoutRoot" Width="920" Height="600" Background="#FFF9EAD6" HorizontalAlignment="Left" VerticalAlignment="Top">
        <Grid.RenderTransform>
            <TransformGroup>
                <ScaleTransform x:Name="RootScaleTransform" ScaleX="1" ScaleY="1" />
                <TranslateTransform x:Name="RootTranslateTransform" X="0" Y="0" />
            </TransformGroup>
        </Grid.RenderTransform>
    </Grid>
</UserControl>
